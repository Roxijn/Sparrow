<opengl>
    <enum name="Error">
        <const name="NoError" value="0x0000"/>
        <const name="InvalidEnum" value="0x0500"/>
        <const name="InvalidValue" value="0x0501"/>
        <const name="InvalidOperation" value="0x0502"/>
        <const name="StackOverflow" value="0x0503"/>
        <const name="StackUnderflow" value="0x0504"/>
        <const name="OutOfMemory" value="0x0505"/>
        <const name="InvalidFramebufferOperation" value="0x0506"/>
        <const name="ContextLost" value="0x0507"/>
    </enum>
    <proto name="GetError">
        <entry>GetError</entry>
        <return type="Error"/>
    </proto>
</opengl>