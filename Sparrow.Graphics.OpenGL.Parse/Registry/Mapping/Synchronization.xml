<opengl>
  <proto name="Flush">
    <entry>glFlush</entry>
  </proto>
  <proto name="Finish">
    <entry>glFinish</entry>
  </proto>
  <proto name="FenceSync">
    <return type="GLsync" />
    <entry>glFenceSync</entry>
    <param name="condition" type="GLenum" />
    <param name="flags" type="GLbitfield" />
  </proto>
  <proto name="DeleteSync">
    <entry>glDeleteSync</entry>
    <param name="sync" type="GLsync" />
  </proto>
  <proto name="ClientWaitSync">
    <return type="GLenum" />
    <entry>glClientWaitSync</entry>
    <param name="sync" type="GLsync" />
    <param name="flags" type="GLbitfield" />
    <param name="timeout" type="GLuint64" />
  </proto>
  <proto name="WaitSync">
    <entry>glWaitSync</entry>
    <param name="sync" type="GLsync" />
    <param name="flags" type="GLbitfield" />
    <param name="timeout" type="GLuint64" />
  </proto>
  <proto name="GetSync">
    <entry>glGetSynciv</entry>
    <param name="sync" type="GLsync" />
    <param name="pname" type="GLenum" />
    <param name="bufSize" type="GLsizei" />
    <param name="length" type="GLsizei*" />
    <param name="values" type="GLint*" />
  </proto>
  <proto name="IsSync">
    <return type="GLboolean" />
    <entry>glIsSync</entry>
    <param name="sync" type="GLsync" />
  </proto>
</opengl>