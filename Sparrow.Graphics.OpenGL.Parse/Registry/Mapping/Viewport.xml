<opengl>
  <proto name="DepthRange">
    <entry>glDepthRange</entry>
    <param name="near" type="GLdouble" />
    <param name="far" type="GLdouble" />
  </proto>
  <proto name="Viewport">
    <entry>glViewport</entry>
    <param name="x" type="GLint" />
    <param name="y" type="GLint" />
    <param name="width" type="GLsizei" />
    <param name="height" type="GLsizei" />
  </proto>
  <proto name="ClipPlane">
    <entry>glClipPlane</entry>
    <param name="plane" type="GLenum" />
    <param name="equation" type="GLdouble*" />
  </proto>
</opengl>